@{
    ViewBag.Title = "NearbyUsers";
}

<h2 style="color:#444; font-size: 14pt;margin-left: 100px; width:200px; border-bottom: 1px solid #aaa">NearbyUsers</h2>

<div id="chat">
@{
    List<int> users = (List<int>) ViewBag.users;
    List<string> names = (List<string>) ViewBag.names;
    for (int i = 0; i < users.Count; ++i)
    {
        <a href="Chat/@(users[i])">@(names[i])</a>
        @Html.DisplayText("<br>");
    }
}
</div>